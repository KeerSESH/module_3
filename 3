def print_params(a=1, b='строка', c=True):
    print(a, b, c)

print_params()
print_params(b= 25)
print_params(c= [1,2,3])
print_params(a= False)
print_params(a= "Name", b= False, c= 11)
print_params(b=2, c=3)

values_list = [2, "string", True]
values_dict = {"a":3, "b":"raw", "c":False}
print_params(*values_list)
print_params(**values_dict)

values_list2 = ["hi", 14]
print_params(*values_list2, 42)
